pragma solidity 0.8.16;

contract DeltaCheckHarness {
  address owner;
  int256 priceCash;

  constructor() {
    owner = msg.sender;
  }

  function setPrice(int256 price_) public {
    priceCash = price_;
  }

  function setOwner(address owner_) external {
    require(msg.sender == owner);
    owner = owner_;
  }

  function getPriceData()
    public
    view
    returns (int256 price, uint256 timestamp)
  {
    price = priceCash;
    timestamp = block.timestamp - 60;
  }

  function getPrice() public view returns (uint256) {
    return uint256(priceCash);
  }
}
